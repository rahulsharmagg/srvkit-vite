{
    "name": "srvkit/vite",
    "version": "0.1.1",
    "description": "Vite Plugin for PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SrvKit\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Rahul Sharma"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": false,
    "keywords": ["vite", "plugin", "php"],
    "homepage": "https://github.com/rahulsharmagg/srvkit-vite",
    "support": {
        "issues": "https://github.com/rahulsharmagg/srvkit-vite/issues",
        "source": "https://github.com/rahulsharmagg/srvkit-vite"
    },
    "require-dev": {
        "phpunit/phpunit": "11.5.x-dev"
    },
    "extra": {
        "codeigniter4": {
            "discoverable": true,
            "modules": [
                "SrvKit\\Vite\\CI"
            ]
        }
    }
}
